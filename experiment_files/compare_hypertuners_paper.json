    "statistics_settings": {
        "minimization": true,
        "cutoff_percentile": 0.96,
        "cutoff_percentile_start": 0.5,
        "cutoff_type": "fevals",
        "objective_time_keys": [
            "all"
        ],
        "objective_performance_keys": [
            "time"
        ]
    },
    "visualization_settings": {
        "plots": [
            {
                "scope": "searchspace",
                "style": "line",
                "x_axis_value_types": [
                    "fevals"
                ],
                "y_axis_value_types": [
                    "normalized",
                    "baseline"
                ]
            },
            {
                "scope": "searchspace",
                "style": "line",
                "x_axis_value_types": [
                    "time"
                ],
                "y_axis_value_types": [
                    "normalized",
                    "baseline"
                ]
            },
            {
                "scope": "search_strategy",
                "style": "heatmap",
                "x_axis_value_types": [
                    "applications"
                ],
                "y_axis_value_types": [
                    "gpus"
                ]
            },
            {
                "scope": "search_strategy",
                "style": "heatmap",
                "x_axis_value_types": [
                    "time"
                ],
                "y_axis_value_types": [
                    "searchspaces"
                ],
                "bins": 100
            },
            {
                "scope": "aggregate",
                "style": "line"
            }
        ],
        "resolution": 1000.0,
        "confidence_level": 0.95,
        "compare_baselines": false,
        "compare_split_times": false
    }
